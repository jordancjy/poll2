<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.apps.poll.dao.extend.SchoolVMMapper">
  <select id="selectAll" resultMap="SchoolVMResultMap">
  select *from poll_school
  </select>
  <resultMap  type="com.briup.apps.poll.bean.extend.SchoolVM"
  id="SchoolVMResultMap">
  
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="logoPath" jdbcType="VARCHAR" property="logopath" />
    <result column="address" jdbcType="VARCHAR" property="address" />
    <result column="telephone" jdbcType="VARCHAR" property="telephone" />
    <result column="copyright" jdbcType="VARCHAR" property="copyright" />
  </resultMap>
  
</mapper>